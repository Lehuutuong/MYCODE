<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/layout_root" android:orientation="vertical"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:padding="10dp">
	<TextView android:layout_marginTop="5dip" android:text="접수인 성명" android:textSize="20sp" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	<EditText android:id="@+id/txtName" android:textSize="20sp" android:layout_width="280dip" android:layout_height="wrap_content" android:layout_marginRight="10dp" />
	<TextView android:layout_marginTop="5dip" android:text="접수인 생년월일" android:textSize="20sp" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	<EditText android:id="@+id/txtBirth" android:textSize="20sp" android:inputType="number" android:layout_width="280dip" android:layout_height="wrap_content" android:layout_marginRight="10dp" />
	<TextView android:layout_marginTop="5dip" android:text="금융상담사 성명" android:textSize="20sp" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	<EditText android:id="@+id/txtCorp" android:textSize="20sp" android:layout_width="280dip" android:layout_height="wrap_content" android:layout_marginRight="10dp" />
	<LinearLayout android:layout_width="match_parent" android:layout_height="10dip"></LinearLayout>
</LinearLayout>